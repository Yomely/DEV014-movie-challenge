{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "plugins": ["import"],
    "overrides": [
    ],
    "rules": {
        "no-console": "warn",
        "import/extensions": 0,
        "no-var": "error",
        "prefer-const": "error",
        "eqeqeq": "error",
        "indent": ["error", 2],
        "import/no-cycle": ["error", { "maxDepth": 1 }],
        "linebreak-style":"off"
      }
}